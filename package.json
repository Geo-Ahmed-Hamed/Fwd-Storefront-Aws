{
    "scripts": {
        "frontend:install": "cd fwd-storefront-aws/fwd-storefront-aws-frontend && npm install -f",
        "frontend:build": "cd fwd-storefront-aws/fwd-storefront-aws-frontend && npm run build",
        "frontend:deploy": "cd fwd-storefront-aws/fwd-storefront-aws-frontend && npm run deploy",
        "backend:install": "cd fwd-storefront-aws/fwd-storefront-aws-backend && npm install -f",
        "backend:build": "cd fwd-storefront-aws/fwd-storefront-aws-backend && npm run build",
        "backend:deploy": "cd fwd-storefront-aws/fwd-storefront-aws-backend && npm run deploy",
        "deploy": "npm run backend:deploy && npm run frontend:deploy"
    }
}
